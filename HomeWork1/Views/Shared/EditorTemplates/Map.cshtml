@model Map

<style>
    #map {
        width: 600px;
        height: 400px;
    }
</style>

<div id="map"></div>
<script>

        var map;
        function initMap() {
          map = new google.maps.Map(document.getElementById('map'), {
            center: {lat: -34.397, lng: 150.644},
            zoom: 8
          });
        }

</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCGIqbTa1KgbU6OTkQljaB7Yos2djufcAE&callback=initMap"
        async defer></script>

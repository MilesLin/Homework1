@*map出不來*@
@{
    Layout = "~/Views/Shared/_OldLayout.cshtml";
    ViewBag.Title = "TestMapWithLayout";
}

<h2>TestMapWithLayout</h2>

    <style>
        #map {
            height: 500px;
            width: 500px;
        }
    </style>

    <div id="map"></div>
    <script>

        var map;
        function initMap() {
          map = new google.maps.Map(document.getElementById('map'), {
            center: {lat: -34.397, lng: 150.644},
            zoom: 8
          });
        }

    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCGIqbTa1KgbU6OTkQljaB7Yos2djufcAE&callback=initMap"
            async defer></script>

@model IEnumerable<HomeWork1.Models.客戶總覽>

@{
    ViewBag.Title = "客戶資料總覽";
}

<h2>客戶資料總覽</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.客戶名稱)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.客戶銀行人數)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.客戶聯絡人數)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.客戶名稱)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.客戶銀行人數)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.客戶聯絡人數)
        </td>
    </tr>
}

</table>
